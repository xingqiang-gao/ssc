<?xml version='1.0' encoding='utf-8'?>
<widget id="jp.co.sej.ssc.mb" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ssc</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
<!--    <content src="index.html" />-->
    <content src="http://localhost:4200" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="loglevel" value="DEBUG" />
    <feature name="FCMPlugin">
        <param name="android-package" value="jp.co.sej.ssc.mb.plugins.fcm.FCMPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="AppSendContent">
        <param name="android-package" value="jp.co.sej.ssc.mb.plugins.asc.AppSendContent" />
    </feature>
    <feature name="SejLogManager">
        <param name="android-package" value="jp.co.sej.ssc.mb.plugins.SejLogManager.SejLogManager.SejLogManager" />
    </feature>
    <feature name="BackgroundHandle">
        <param name="android-package" value="jp.co.sej.ssc.mb.plugins.bh.BackgroundHandle" />
    </feature>
</widget>
